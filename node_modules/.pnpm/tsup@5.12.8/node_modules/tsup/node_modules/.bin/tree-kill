#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/tree-kill@1.2.2/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules:/Users/oscar/Documents/misc/node_modules:/Users/oscar/Documents/node_modules:/Users/oscar/node_modules:/Users/node_modules:/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/tsup@5.12.8/node_modules/tsup/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/tsup@5.12.8/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/tree-kill@1.2.2/node_modules/tree-kill/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/tree-kill@1.2.2/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules:/Users/oscar/Documents/misc/node_modules:/Users/oscar/Documents/node_modules:/Users/oscar/node_modules:/Users/node_modules:/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/tsup@5.12.8/node_modules/tsup/node_modules:/Users/oscar/Documents/misc/tsup-repro/node_modules/.pnpm/tsup@5.12.8/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tree-kill@1.2.2/node_modules/tree-kill/cli.js" "$@"
else
  exec node  "$basedir/../../../../../tree-kill@1.2.2/node_modules/tree-kill/cli.js" "$@"
fi
